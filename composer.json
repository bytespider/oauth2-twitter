{
    "name": "bytespider/oauth2-twitter",
    "description": "Twitter OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Rob Griffiths",
            "email": "rob@bytespider.eu"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authentication",
        "twitter"
    ],
    "require": {
        "league/oauth2-client": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "League\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "League\\OAuth2\\Client\\Test\\": "tests/src/"
        }
    }
}
